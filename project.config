[access]
	inheritFrom = chromiumos-nosticky
[submit]
	action = inherit
	mergeContent = true
[project]
	description = Google's Verified Boot reference implementation (versions 1.x & 2.x) and helper tools
[access "refs/*"]
	owner = group Administrators
[access "refs/for/refs/heads/master"]
	exclusiveGroupPermissions = push
[access "refs/heads/master"]
	exclusiveGroupPermissions = push
[receive]
	requireContributorAgreement = false
[plugin "zosscoverage"]
	enable = false
	enableLowCoverageAlerting = false
	enableVoting = false
	incrementalCoverageDisplayErrorBelow = 0
	incrementalCoverageDisplayWarningBelow = 0
	labelName = Code-Coverage
	minAffectedFilesToEnforce = 0
	minAffectedLinesToEnforce = 0
	negativeVoteValue = -1
	positiveVoteValue = 1
[plugin "uploadvalidator"]
	blockedContentTypeWhitelist = false
	maxPathLength = 0
	rejectDuplicatePathnamesLocale = en
	rejectSubmodule = false
	rejectSymlink = false
	rejectWindowsLineEndings = false
